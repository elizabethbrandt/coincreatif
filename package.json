    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "firebase": "^8.3.0",
    "fuse.js": "^6.4.6",
    "if-env": "^1.0.4",
    "material-ui-image": "^3.3.1",
    "mongoose": "^5.12.2",
